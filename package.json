{
  "name": "greenhouse-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "eslint \"src/**/*.{js,ts,html}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{js,ts,html}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,html,scss,css,json}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^20.2.0",
    "@angular/animations": "19.2.11",
    "@angular/common": "19.2.11",
    "@angular/compiler": "19.2.11",
    "@angular/core": "19.2.11",
    "@angular/forms": "19.2.11",
    "@angular/platform-browser": "19.2.11",
    "@angular/platform-browser-dynamic": "19.2.11",
    "@angular/router": "19.2.11",
    "@primeng/themes": "19.1.3",
    "chart.js": "^4.5.0",
    "date-fns": "4.1.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "ngx-cookie-service": "19.1.2",
    "ngx-json-viewer": "^3.2.1",
    "prettier": "3.5.3",
    "primeicons": "7.0.0",
    "primeng": "19.1.3",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.11",
    "@angular-eslint/eslint-plugin": "19.4.0",
    "@angular-eslint/eslint-plugin-template": "19.4.0",
    "@angular/cli": "19.2.11",
    "@angular/compiler-cli": "19.2.11",
    "@angular/localize": "19.2.11",
    "@eslint/js": "9.27.0",
    "@types/jasmine": "5.1.8",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.27.0",
    "globals": "16.1.0",
    "husky": "^9.1.7",
    "jasmine-core": "5.7.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "^16.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json}": [
      "prettier --write"
    ]
  }
}
